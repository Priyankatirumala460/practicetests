package practicetests;

import java.util.Scanner;

public class MaximumOf {

	public static void main(String[] args) {
		
		Scanner number = new Scanner(System.in);
		System.out.println("Enter 3 digits");
		
		int inputNumber1 = number.nextInt();
		int inputNumber2 = number.nextInt();
		int inputNumber3 = number.nextInt();
		number.close();
		
		if(inputNumber1 > inputNumber2 && inputNumber2 > inputNumber3) {
			System.out.println("highest of three numbers is " +inputNumber1);
			
		}else if(inputNumber2 > inputNumber1 && inputNumber2 > inputNumber3){
				
				System.out.println("highest of three numbers is " +inputNumber2);
	
			}else if (inputNumber3 > inputNumber1 && inputNumber3 > inputNumber2) {
						System.out.println("highest number is " + inputNumber3);
	
			}
			else {
				System.out.println("All 3 numbers are equal");
			}
	
	}
}


	

		



public class Test {
    public static void main(String[] args) {
        int i = 10;
        int j = 7;

        // Swap values without using a third variable
        i = i + j;
        j = i - j;
        i = i - j;

        System.out.println("After swapping:");
        System.out.println("i = " + i);
        System.out.println("j = " + j);
    }
}

package practicetests;

import java.util.Scanner;

public class Assignment{
 public static void main(String[] args){

Scanner value = new Scanner(System.in);

while(true) {
	
System.out.println("Enter your input");

int input = value.nextInt();
int input2 = value.nextInt() ;
 
int sum = input + input2;
System.out.println("sum" +sum);

int diff = input - input2;
System.out.println("diff"+diff);

  if (input > input2 ) {
System.out.println("max" +input);
System.out.println("min"+input2);

  }else {
	  System.out.println("min"+input);
	  System.out.println("max"+input2);
	  
  }
  
}

}
}
